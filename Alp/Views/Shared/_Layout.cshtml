<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/Css")

    <script type="text/javascript" src="~/Scripts/jquery-1.9.1.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-ui-1.10.2.custom.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.slides.min.js"></script>
    <script type="text/javascript" src="~/Scripts/fullcalendar.js"></script>
    <script type="text/javascript" src="~/Scripts/schoolcalendar.js"></script>

    <!--[if lt IE 9]>
    <script src="@Url.Content("~/Scripts/css3-mediaqueries.js")"></script>
    <script src="@Url.Content("~/Scripts/html5.js")"></script>
    <![endif]-->
</head>
<body>
    <div class="navbar">
        <div class="navbar-inner" id="navbar">
            <ul class="nav">
                <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                <li><a href="@Url.Action("StaffAndFaculty", "Home")">Staff & Faculty</a></li>
                <li><a href="#">Registration Information</a></li>
                <li><a href="@Url.Action("SchoolCalendar", "Home")">School Calendar</a></li>
                <li><a href="@Url.Action("Directions", "Home")">Directions</a></li>
                <li><a href="@Url.Action("ContactUs", "Home")">Contact Us</a></li>
            </ul>
        </div>
    </div>
    
    @RenderBody()

    <br /><br />
    <div class="line-break"></div>
    <br /><br />
    <div class="footer">
        <div class="container">
            <div class="row span12">
                <div class="span3"></div>
                <div class="span4">
                    Copyright &copy; 2013 Ascension Lutheran Preschool
                </div>
                <div class="span5"></div>
            </div>
        </div>
        
    </div>
</body>
</html>
