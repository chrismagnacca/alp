<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <!--[if lt IE 10]>
	<script src="@Url.Content("~/Scripts/css3-mediaqueries.js")"></script>
	<script src="@Url.Content("~/Scripts/html5.js")"></script>
    <![endif]-->
</head>
<body>
    <div class="container">
        <header>
            <h1> Acension Lutheran Preschool & Extended Care</h1>
        </header>

        <nav class="nav">
            <ul>
		        <li class="current"><a href="#">Home</a></li>
		        <li><a href="#">Our Mission & Program</a></li>
		        <li><a href="#">Staff & Faculty</a></li>
		        <li><a href="#">Registration Information</a></li>
		        <li><a href="#">Location & Directions</a></li>
	        </ul>
        </nav>
        <div class="content">
            @RenderBody()
        </div>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)

</body>
</html>
