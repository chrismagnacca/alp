<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")

    <style>
        /* Prevents slides from flashing */
        #slides {
          display:none;
        }
    </style>
    
    <script type="text/javascript" src="~/Scripts/jquery-1.9.1.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.slides.min.js"></script>
    <!--[if lt IE 10]>
	<script src="@Url.Content("~/Scripts/css3-mediaqueries.js")"></script>
	<script src="@Url.Content("~/Scripts/html5.js")"></script>
    <![endif]-->
</head>
<body>
    <div class="container">
        <header>
            <h1> Acension Lutheran Preschool</h1>
        </header>

        <nav class="nav">
            <ul>
		        <li><a href="@Url.Action("Index","Home")">Home</a></li>
		        <li><a href="#">Our Program</a></li>
		        <li><a href="#">Staff & Faculty</a></li>
		        <li><a href="#">Registration</a></li>
                <li><a href="#">Event Calendar</a></li>
		        <li><a href="@Url.Action("Directions","Home")">Directions</a></li>
                <li><a href="#">Contact Us</a></li>
	        </ul>
        </nav>

        <div class="content">
            @RenderBody()
        </div>

        <div class="footer">
            ascensionpreschool@hotmail.com | 1479 Morse Rd. Columbus, OH 43229  | Office: (614) 268-2010 | Fax: (614) 268-1164 
        </div>
        <br />
        <br />
        <br />
    </div>



</body>
</html>
